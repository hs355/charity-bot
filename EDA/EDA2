   "source": [
    "### Text Preprocessing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def remove_stop_words(text):\n",
    "    text = text.split()\n",
    "    stops = set(stopwords.words(\"english\"))\n",
    "    text = [w for w in text if not w in stops]\n",
    "    text = \" \".join(text)\n",
    "    return text\n",
    "\n",
    "def remove_punctuation(text):\n",
    "    tokenizer = RegexpTokenizer(r'\\w+')\n",
    "    text = tokenizer.tokenize(text)\n",
    "    text = \" \".join(text)\n",
    "    return text\n",
    "\n",
    "def make_lower_case(text):\n",
    "    return text.lower()\n",
    "\n",
    "def removeNonAscii(s):\n",
    "    return \"\".join(i for i in s if  ord(i)<128)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['cleaned_missionStatement'] = df['missionStatement'].apply(removeNonAscii)\n",
    "df['cleaned_missionStatement'] = df.missionStatement.apply(func = remove_stop_words)\n",
    "df['cleaned_missionStatement'] = df.missionStatement.apply(func = remove_punctuation)\n",
    "df['cleaned_missionStatement'] = df.missionStatement.apply(func = make_lower_case)"
   ]
  },
