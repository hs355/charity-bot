    "### Distribution of Main Words for the Mission Statements"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<matplotlib.axes._subplots.AxesSubplot at 0x7fe755be6290>"
      ]
     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1080x504 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Distribution for Mission Statements\n",
    "\n",
    "# Convert Mission Statements into Vectors using TF-IDF with Trigrams\n",
    "tf = TfidfVectorizer(ngram_range=(1, 1), stop_words='english', lowercase = False)\n",
    "tfidf_matrix = tf.fit_transform(df['cleaned_missionStatement'])\n",
    "total_words = tfidf_matrix.sum(axis=0) \n",
    "\n",
    "# Find Word Frequencies\n",
    "freq = [(word, total_words[0, idx]) for word, idx in tf.vocabulary_.items()]\n",
    "freq = sorted(freq, key = lambda x: x[1], reverse = True)\n",
    "\n",
    "# Convert to DataFrame \n",
    "trigram = pd.DataFrame(freq)\n",
    "trigram.rename(columns = {0:'trigram', 1: 'count'}, inplace = True) \n",
    "\n",
    "# Plot the Trigram Distribution for First 25 Records\n",
    "trigram = trigram.head(25)\n",
    "trigram.plot(x ='trigram', y = 'count', kind = 'bar', title = \"Word Distribution\", figsize = (15,7), )"
   ]
  },
