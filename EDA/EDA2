    "# Distribution of Part-of-Speech Tags in Mission Statements\n",
    "blob = TextBlob(str(df['missionStatement']))\n",
    "pos_df = pd.DataFrame(blob.tags, columns = ['word' , 'pos'])\n",
    "pos_df = pos_df.pos.value_counts()[:20]\n",
    "\n",
    "pos_df.plot(kind = 'bar', figsize=(10, 8), title = \"Top 20 Part-of-Speech Tags for Mission Statements\")"
